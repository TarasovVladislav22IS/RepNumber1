Module Module1

    Sub Main()
        Dim x, a, b, c, y As Double
        x = 1.18
        a = 1.3
        b = -1.5
        c = 2.84
        If x + a * b > Math.Pow(b, 2) Then
            y = a * x + b * Math.Pow(x, 2) - c
        ElseIf x + a * b = Math.Pow(b, 2) Then
            y = a * x + Math.Sqrt(x + 1)
        ElseIf x + a * b < Math.Pow(b, 2) Then
            y = a + b * x / Math.Sqrt(x + 1)
        End If
        Console.ReadKey()
    End Sub

End Module
